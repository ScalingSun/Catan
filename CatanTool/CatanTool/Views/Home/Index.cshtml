@model CatanTool.Models.Playboard

@{
    ViewData["Title"] = "Home Page";
}

<div id="TestImage">
    <img height="1000" width="1000" src=@Model.Data>
</div>
<button onclick="(saveBase64AsFile(@Model.Data,Image))">smegma</button>

<ol>
    @foreach (var junction in Model.TopJunctions)
    {
        <li>
            @junction.ToString();
        </li>
    }
</ol>


<script>

   function saveBase64AsFile(base64, fileName) {

        var link = document.createElement("a");

        link.setAttribute("href", base64);
        link.setAttribute("download", fileName);
        link.click();
    }
</script>

<br />

